                    <attr_swatch_option_color>868686</attr_swatch_option_color>
                    <swatch_option_text_color>646464</swatch_option_text_color>
                    <swatch_option_hover_color>333333</swatch_option_hover_color>
                </swatch>
                <price>
                    <price_color>333333</price_color>
                    <old_price_color>868686</old_price_color>
                </price>
                <subscribe>
                    <subscribe_bg_color>e6e6e6</subscribe_bg_color>
                    <subscribe_color>575757</subscribe_color>
                </subscribe>
                <footer>
                    <footer_bg_color>ffffff</footer_bg_color>
                </footer>
                <copyright>
                    <copyright_bg_color>343434</copyright_bg_color>
                    <copyright_color>cdcdcd</copyright_color>
                </copyright>
            </color_scheme>
            <gallery>
                <thumbs_direction>horizontal</thumbs_direction>
            </gallery>
        </mfthemefo>
    </default>
</config>
